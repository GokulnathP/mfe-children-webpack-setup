{
  "name": "children",
  "version": "1.0.0",
  "description": "Boilerplate code for bootstrap react micro frontend application",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve -c config/webpack/webpack.config.dev.js",
    "start:mfe": "webpack serve -c config/webpack/webpack.config.mfe.js",
    "build": "rm -rf dist && webpack -c config/webpack/webpack.config.prod.js",
    "test": "jest --ci -c config/jest.config.json",
    "test:watch": "jest --watch -c config/jest.config.json",
    "test:coverage": "jest --coverage -c config/jest.config.json",
    "lint": "eslint -c config/.eslintrc ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint -c config/.eslintrc ./src --ext .js,.jsx,.ts,.tsx --fix",
    "stylelint": "stylelint --config config/.stylelintrc './src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "chmod +x config/setup-husky.sh && ./config/setup-husky.sh"
  },
  "keywords": [
    "MFE",
    "React",
    "Boilerplate",
    "Micro frontent"
  ],
  "author": "GokulnathP",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "graphql": "^16.6.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@emotion/jest": "^11.10.5",
    "@graphql-tools/jest-transform": "^1.2.2",
    "@graphql-tools/webpack-loader": "^6.7.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "dynamic-container-path-webpack-plugin": "^1.2.0",
    "es6-promise-promise": "^1.0.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-resolve": "^29.3.1",
    "jest-watch-typeahead": "^2.2.0",
    "jest-when": "^3.5.2",
    "lint-staged": "^13.0.4",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.0",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "type-fest": "^3.3.0",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "yarn lint:fix",
      "yarn stylelint",
      "yarn test --findRelatedTests --coverageThreshold --passWithNoTests '{}'"
    ]
  },
  "prettier": "./config/.prettierrc.js"
}
